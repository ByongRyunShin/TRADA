            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="David Jones"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
